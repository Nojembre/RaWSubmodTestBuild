				<!-- We can block/redirect shots from the following units types: -->
				<Applicable_Unit_Categories> Infantry | Vehicle</Applicable_Unit_Categories>
				<Applicable_Unit_Types>Asajj_Ventress, Aurra_Sing, Bossk, Cad_Bane, Count_Dooku, Durge, General_Grievous, Darkside_Adepts, Squad_Generic_Field_Commander_Cis, Bx_Commando</Applicable_Unit_Types>
